{
  "name": "@figma-story-plugin/storybook-addon",
  "version": "0.1.0",
  "description": "Storybook addon for token extraction",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./manager": "./dist/manager.js",
    "./preset": "./dist/preset.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "storybook",
    "addon",
    "figma",
    "tokens"
  ],
  "author": "rgehrkedk",
  "license": "MIT",
  "dependencies": {
    "@figma-story-plugin/shared": "workspace:*",
    "@figma-story-plugin/parser": "workspace:*",
    "@figma-story-plugin/bridge": "workspace:*",
    "@storybook/addon-kit": "^7.6.10",
    "@storybook/api": "^7.6.10",
    "@storybook/components": "^7.6.10",
    "@storybook/core-events": "^7.6.10",
    "@storybook/manager-api": "^7.6.10",
    "@storybook/theming": "^7.6.10",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@storybook/blocks": "^7.6.10",
    "@storybook/components": "^7.6.10",
    "@storybook/core-events": "^7.6.10",
    "@storybook/manager-api": "^7.6.10",
    "@storybook/preview-api": "^7.6.10",
    "@storybook/theming": "^7.6.10",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
